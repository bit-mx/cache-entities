{
    "name": "bit-mx/cache-entities",
    "description": "Creates cacheable entities",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "BitMx\\CacheEntities\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BitMx\\CacheEntities\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jorge Enr√≠quez",
            "email": "44071711+jenriquez-bit@users.noreply.github.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0 || ^12.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.15",
        "phpstan/extension-installer": "^1.3",
        "larastan/larastan": "^3.0",
        "tomasvotruba/type-coverage": "^2.0.2",
        "orchestra/testbench": "^v10.4.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BitMx\\CacheEntities\\CacheEntitiesServiceProvider"
            ]
        }
    }
}
